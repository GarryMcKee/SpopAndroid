<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.garrymckee.spop.UI.SpopMainActivity">

    <Button
        android:id="@+id/generate_user_taste_profile_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/generate_user_profile_button" />

    <Button
        android:id="@+id/print_user_taste_profile_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/print_user_taste_button_text"/>
</LinearLayout>
